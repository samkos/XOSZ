@echo off
echo running zephyr on $2 nodes using dataset ADAT/zephyr$1 ...

cas=$1
p=$2

echo $p > ./zephyr.in
sed 's|xxxxx/bip|./output|g;s/\!.*$//g' ../ADAT/zephyr$cas.dat >> ./zephyr.in


date
echo ================== running ==========================
echo running... 
pwd
ls -l
cp ../SRC/zephyr .
./zephyr < zephyr.in > zephyr.out
date
echo
echo ==================== output ==========================
tail -25 zephyr.out
echo ================ fin output ==========================